<% @collaborators.each do |collaborator| %>
  $('.js-collaborators-container').append('<%= j render('collaborator', collaborator: collaborator) %>');
<% end %>

$('.js-more-collaborators').replaceWith('<%= escape_javascript(
  link_to_next_page @collaborators, t('.show-more'),
  remote: true,
  class: 'js-more-colaborators btn btn-primary btn-lg') %>');
