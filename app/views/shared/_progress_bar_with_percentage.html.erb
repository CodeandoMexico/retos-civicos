<%= content_tag :div, class: 'progress' do %>
  <%= content_tag :div, humanize_percentage(value_now, total_value), class:'progress-bar', role:'progressbar', "aria-valuenow"=> "#{compute_percentage(value_now, total_value)}", "aria-valuemin"=> '0', "aria-valuemax"=> '100', style:"min-width: 3em; width:#{humanize_percentage(value_now, total_value)};"  %>
<% end %>
